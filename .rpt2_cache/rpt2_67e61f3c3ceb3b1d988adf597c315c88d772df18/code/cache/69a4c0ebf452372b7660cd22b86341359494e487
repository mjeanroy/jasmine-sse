{"code":"import { assign } from '../../../src/core/common/assign';\r\ndescribe('assign', function () {\r\n    it('should create target object from sources object', function () {\r\n        var target = {};\r\n        var source1 = { id: 1 };\r\n        var source2 = { id: 2, name: 'John Doe' };\r\n        var result = assign(target, source1, source2);\r\n        expect(result).toBe(target);\r\n        expect(result).toEqual({\r\n            id: 2,\r\n            name: 'John Doe',\r\n        });\r\n    });\r\n});\r\n//# sourceMappingURL=assign.test.js.map","map":"{\"version\":3,\"file\":\"assign.test.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../test/core/common/assign.test.ts\"],\"names\":[],\"mappings\":\"AAwBA,OAAO,EAAC,MAAM,EAAC,MAAM,iCAAiC,CAAC;AAEvD,QAAQ,CAAC,QAAQ,EAAE;IACjB,EAAE,CAAC,iDAAiD,EAAE;QACpD,IAAM,MAAM,GAAG,EAAE,CAAC;QAClB,IAAM,OAAO,GAAG,EAAC,EAAE,EAAE,CAAC,EAAC,CAAC;QACxB,IAAM,OAAO,GAAG,EAAC,EAAE,EAAE,CAAC,EAAE,IAAI,EAAE,UAAU,EAAC,CAAC;QAE1C,IAAM,MAAM,GAAG,MAAM,CAAC,MAAM,EAAE,OAAO,EAAE,OAAO,CAAC,CAAC;QAEhD,MAAM,CAAC,MAAM,CAAC,CAAC,IAAI,CAAC,MAAM,CAAC,CAAC;QAC5B,MAAM,CAAC,MAAM,CAAC,CAAC,OAAO,CAAC;YACrB,EAAE,EAAE,CAAC;YACL,IAAI,EAAE,UAAU;SACjB,CAAC,CAAC;IACL,CAAC,CAAC,CAAC;AACL,CAAC,CAAC,CAAC\"}"}
